   ESPECIFICAÇÃO DE TIPOS E ENUMERAÇÕES (DOMÍNIOS), UTILIZADOS NOS ARQUIVOS ENVIADO PELO CONCENTRADOR DE DADOS PORTUÁRIOS
  -->

  <!-- Início da definição de tipos -->

    <!-- tipo que aceita inteiros e null (em branco) -->
    <xsd:simpleType name="psp_integer">
        <xsd:restriction base="xsd:string">
        <xsd:pattern value="([\-|\+]?[0-9]+)*"/>
        <xsd:minLength value="0"/>
        <xsd:maxLength value="9"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!-- tipo que aceita inteiros e null (em branco) -->
    <xsd:simpleType name="psp_long">
        <xsd:restriction base="xsd:string">
        <xsd:pattern value="([\-|\+]?[0-9]+)*"/>
        <xsd:minLength value="0"/>
        <xsd:maxLength value="18"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- tipo que aceita decimais e null (em branco) -->
    <xsd:simpleType name="psp_decimal">
        <xsd:restriction base="xsd:string">
        <xsd:pattern value="([\-|\+]?([0-9]+[.])?[0-9]+)?"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!-- tipo que aceita decimais e null (em branco) -->
    <xsd:simpleType name="psp_double">
        <xsd:restriction base="xsd:string">
        <xsd:pattern value="([\-|\+]?([0-9]+[.])?[0-9]+)?"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!-- tipo que aceita boolean e null (em branco) -->
        <xsd:simpleType name="psp_boolean">
        <xsd:restriction base="xsd:string">
        <xsd:pattern value="true|false|1|0|"/>
        </xsd:restriction>
    </xsd:simpleType>

  <!-- Fim da definição de tipos específicos do PSP -->



